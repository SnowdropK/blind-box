}

/* 弹窗 */
.dialogMak {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.8);
}
.dialog {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
  width: 500rpx;
  box-sizing: border-box;
  padding: 49rpx 46rpx 43rpx;
}
.dialog .button {
  margin-top: 85rpx;
  margin-left: 50%;
  transform: translate(-50%, 0);
  width: 180rpx;
  height: 80rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  background-image: linear-gradient(-90deg, #4a7cf7, #7da8f6);
  /* background-image: linear-gradient(-90deg, #E99411, #F2BF70); */
}
.dialog .dialogTitle {
  font-size: 30rpx;
  text-align: center;
}

/* 领取奖品弹窗 */
.dialogReceiveBc {
  background-image: url("data:image/png;base64,");
}
.dialog .prizeList {
  display: flex;
  flex-direction: column;
  margin-top: 46rpx;
  padding: 0 22rpx;
}
.dialog .prizeList .prize {
  display: flex;
  justify-content: space-between;
  margin-top: 36rpx;
}
.dialog .prizeList .prizeBox {
  display: flex;
}
.dialog .prizeBox .prizeImg {
  /* width: 80rpx;
    height: 80rpx; */
  border-radius: 10rpx;
  padding: 12rpx;
}
.dialog .prizeBox .prizeImg .icon {
  width: 60rpx;
  height: 52rpx;
}

.dialog .prizeBox .prizeTitle2 {
  margin-top: 5rpx;
  margin-left: 15rpx;
  width: 180rpx;
  height: 68rpx;
}
.dialog .prize .prizeNum {
  margin-top: 5rpx;
  color: #e3405b;
}
