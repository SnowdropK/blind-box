page {
  background-color: #18181d;
}

.commonTop {
  padding-top: 160rpx;
}
.commonTop .headImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 366rpx;
}
.commonScrollBox {
  top: 90rpx;
  left: 0.3rem;
  padding-top: 0.4rem;
  width: 690rpx;
  height: 1080rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}
.commonWrap {
  top: 376rpx;
  /* background-color: #3C3C3C; */
}
.pointStoreHead {
  position: absolute;
  top: 188rpx;
  left: 0;
  width: 750rpx;
  height: 116rpx;
}
.pointStoreHead .imageHead {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
}
.storeNum {
  width: 400rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 175rpx;
  color: #FDB300;
  font-size: 30rpx;
  text-align: center;
  overflow: hidden;
}
.storeBtn {
  position: absolute;
  bottom: -2rpx;
  left: 325rpx;
  width: 100rpx;
  height: 30rpx;
  color: #A8A8A8;
  font-size: 20rpx;
  text-align: center;
}

.pointInfoImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 1325rpx;
}

.store-right-box{
  position: absolute;
  top: 145rpx;
  right: 100rpx;
  display: flex;
  flex-direction: column;
}

.store-btn{
  width: 148rpx;
  height: 48rpx;
  border-radius: 24rpx;
  border: 1rpx solid #FFFFFF;
  font-size: 24rpx;
  color: #ffffff;
  text-align: center;
  line-height: 48rpx;
  margin-bottom: 16rpx;
}

.my-store-title{
  text-align: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #FFB400;
  line-height: 33px;
  padding-bottom: 30rpx;
}

.store-grid-box {
  display: flex;
  flex-wrap: wrap;
  margin-left: 34rpx;
  padding-bottom: 20rpx;
}
.store-grid-box.coupon {
  margin-left: 42rpx;
}

.store-postcard-item {
  width: 190rpx;
  height: 196rpx;
  margin-right: 26rpx;
  margin-bottom: 80rpx;
  position: relative;
  /* overflow: hidden; */
  display: flex;
  flex-direction: column;
  background-image: url('data:image/png;base64,');
}
.store-postcard-item:nth-child(3n) {
  margin-right: 0;
}

.store-postcard-item .bg{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 308rpx;
  height: 344rpx;
}

.store-postcard-item .new{
  position: absolute;
  top: 18rpx;
  left: 14rpx;
  width: 54rpx;
}

.store-postcard-item .use{
  position: absolute;
  bottom: 12rpx;
  right: 20rpx;
  width: 96rpx;
  height: 48rpx;
}
.store-postcard-item .store-num, .store-num-img {
  position: absolute;
  top: 1rpx;
  right: 0;
  width: 90rpx;
  height: 47rpx;
  overflow-x: hidden;
}
.store-num-text {
  color: #000000;
  font-size: 22rpx;
  line-height: 47rpx;
  text-align: center;
}

.point-box {
  position: absolute;
  bottom: -18rpx;
  left: 36rpx;
  width: 117rpx;
  height: 42rpx;
  text-align: center;
}

.point-box .num{
  font-size: 29rpx;
  font-weight: bold;
  color: #000000;
  padding-top: 6rpx;
}

.store-postcard-item .point-box .point-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 117rpx;
  height: 41rpx;
}

.store-postcard-item .info-img{
  width: 130rpx;
  height: 90rpx;
  margin-left: 32rpx;
  margin-top: 22rpx;
}

.store-postcard-item .info-name{
  width: 160rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 16rpx;
  padding-left: 20rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 500;
  color:#F3AC00;
  white-space: nowrap;
}

.exchangeBtn {
  position: absolute;
  bottom: -80rpx;
  right: 14rpx;
  width: 128rpx;
  height: 48rpx;
  color: #DADADA;
  font-size: 24rpx;
}

.filterBox {
  margin: 20rpx 27rpx;
  width: 690rpx;
  height: 50rpx;
  display: flex;
  align-content: flex-end;
  position: relative;
  z-index: 9;
}

.filterItem {
  width: 190rpx;
  height: 55rpx;
  line-height: 55rpx;
  margin-right: 8rpx;
  font-size: 23rpx;
  color: #cccccc;
  text-align: center;
  background-image: url('data:image/png;base64,');
}

.filterItem.act {
  color:#F7B52D;
}
.store-coupon {
  width: 152rpx;
  height: 192rpx;
  margin-right: 70rpx;
  margin-bottom: 80rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  background-image: url('data:image/png;base64,');
}
.store-coupon:nth-child(3n) {
  margin-right: 0;
}

.store-coupon .point-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 117rpx;
  height: 41rpx;
}
.store-coupon .point-box {
  left: 17rpx;
  bottom: 14rpx;
}
.store-coupon .point-box .num {
  color: #DB8900;
}
.store-coupon .coupon-text {
  font-size: 44rpx;
  font-weight: bold;
  text-align: center;
  line-height: 50rpx;
  margin-top: 12rpx;
}
.coupon-info-text, .store-coupon .info-name {
  font-size: 22rpx;
  text-align: center;
  margin-top: 8rpx;
}
.store-coupon .exchangeBtn {
  width: 152rpx;
  text-align: center;
  right: 0;
  bottom: -70rpx;
}

.text-hide2 {
	word-break: break-all;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}