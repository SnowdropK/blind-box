page {
  background-color:#3C3C3C;
  height: 100vh;
}

.collectTotal {
  position: absolute;
  top: 202rpx;
  font-size: 24rpx;
  color: #FFC000;
  text-align: center;
  width: 100%;
}
.collectTotal text {
  font-size: 28rpx;
  color: #ffffff;
  margin: 0 2rpx;
}

.commonWrapNew {
  top: 251rpx;
  background-color: #3C3C3C;
}

.titlelabel {
  font-size: 32rpx;
  font-weight: bold;
  color: #FFFFFF;
  line-height: 150rpx;
  padding-left: 30rpx;
  letter-spacing: 3rpx;
  margin-top: 120rpx;
}

.commonScrollBox {
  top: 30rpx;
  padding-bottom: 40rpx;
}

.box {
  width: 684rpx;
  height: 180rpx;
  border-radius: 4rpx;
  margin: 30rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* padding: 10rpx 30rpx 10rpx 10rpx; */
  position: relative;
  background-image: url('data:image/png;base64,');
}

.ipImg {
  width: 180rpx;
  height: 180rpx;
  position: relative;
}

.ipInfo {
  width: 450rpx;
  height: 140rpx;
  position: relative;
  margin: 20rpx;
}

.ipName {
  width: 100%;
  font-size: 28rpx;
  color: #ffffff;
  padding-bottom: 15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.labels {
  width: 100%;
  overflow: hidden;
}

.label {
  float: left;
  width: 90rpx;
  height: 34rpx;
  padding: 2rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #FFFFFF;
  line-height: 32rpx;
  text-align: center;
  margin-right: 10rpx;
  background-image: url('data:image/png;base64,');
}

