.numBox {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.num1 {
  font-size: 24rpx;
  color: #FFBF00;
  letter-spacing: 3rpx;
}

.num1 text {
  font-size: 38rpx;
  font-weight: bold;
}

.sellOutBox {
  position: absolute;
  top: -2rpx;
  left: 0;
  width: 100%;
  height: 182rpx;
  /* background: rgba(0, 0, 0, 0.5); */
  background-image: url('data:image/png;base64,');
}

.sellOutImg {
  width: 114rpx;
  height: 82rpx;
  margin: 50rpx 0 0 30rpx;
}

.delBtn {
  position: absolute;
  top: -15rpx;
  right: -10rpx;
  width: 40rpx;
  height: 44rpx;
  z-index: 9;
}