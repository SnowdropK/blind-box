page {
  background-color: #18181d;
}

.commonTop {
  padding-top: 160rpx;
}

.commonTop .headImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 366rpx;
}

.commonWrap {
  top: 376rpx;
}

.commonfilterBox {
  top: 303rpx;
  right: 0;
  width: 520rpx;
  height: 104rpx;
  background-image: url('data:image/png;base64,');
}

.pointfilterItem {
  width: auto;
  height: 0.72rem;
  font-size: 0.26rem;
  color:#A5A5A5;
  line-height: 0.72rem;
  margin-right: 0.32rem;
  position: relative;
}
.pointfilterItem.act {
  color: #E79800;
}

.pointHead {
  position: absolute;
  top: 188rpx;
  left: 0;
  width: 750rpx;
  height: 94rpx;
}
.pointHead .imageHead {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
}
.pointHead .pointNum {
  width: 400rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 175rpx;
  color: #FDB300;
  font-size: 30rpx;
  text-align: center;
  overflow: hidden;
}

.pointInfoImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 1325rpx;
}

.commonRecordItem {
  margin-left: 0;
  padding-left: 50rpx;
  padding-right: 0;
  background-color: transparent;
}
.costItemLine, .itemLine {
  position: absolute;
  left: 0;
  width: 10rpx;
  height: 82rpx;
}
.costItemLine {
  background-color: #7F7F7F;
}
.itemLine {
  background-color: #FDB300;
}
.commonRecordtype {
  color: #FDB300;
}
.commonRecordtime {
  color: #A1A1A1;
}
.commonRecordinfo {
  width: 60%;
}
.commonRecordnum {
  width: 40%;
  color: #ffffff;
}