page {
  background-color: #18181d;
}

.commonTop {
  padding-top: 160rpx;
}
.commonTop .headImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 317rpx;
}
.commonWrapNew {
  top: 300rpx;
}
.cardHead {
  width: 750rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.cardHeadLeft, .cardHeadRight {
  width: 239rpx;
  height: 71rpx;
  line-height: 71rpx;
  font-size: 24rpx;
  text-align: center;
}
.cardHeadLeft {
  color: #FFBA00;
  background-image: url('data:image/png;base64,');
}
.cardHeadLeft text {
  margin-left: 28rpx;
}
.cardHeadRight {
  color: #FFBA00;
  background-image: url('data:image/png;base64,');
}
.cardHeadRight text {
  margin-right: 28rpx;
}

.cardInfoImg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 1325rpx;
}

.commonScrollBox{
  padding-bottom: 100rpx;
}

.my-postcard-title{
  text-align: center;
  font-size: 48rpx;
  font-weight: 500;
  color: #000838;
  line-height: 33px;
  padding-bottom: 40rpx;
}

.postcard-item{
  margin-bottom: 12rpx;
  width: 690rpx;
  height: 85rpx;
  position: relative;
}

.postcard-item .bg{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 750rpx; 
  height: 82rpx;
}

.postcard-item .use{
  position: absolute;
  right: 26rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 106rpx;
  height: 48rpx;
}

.postcard-item .img{
  position: absolute;
  left: 50rpx;
  top: 44%;
  transform: translateY(-50%);
  width: 102rpx;
  max-height: 70rpx;
}

.postcard-item .info{
  position: absolute;
  left: 220rpx;
  top: 0;
  bottom: 0;
  width: 300rpx;
  height: 70rpx;
  color:#FFB400;
  font-size: 27rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.postcard-item .num{
  position: absolute;
  right: 0;
  width: 150rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #ffffff;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  overflow: hidden;
}

.text-hide {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
}