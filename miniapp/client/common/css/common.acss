/* 基础样式 */
view {
  word-break: break-word;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-origin: padding-box;
  background-clip: padding-box;
  line-height: 1;
  box-sizing: border-box;
  display: block;
  position: relative;
}

/* 公用样式 */

page {
  background-color: #fcf6f4;
  height: 100%;
}

button {
  border: none !important;
  background: none !important;
  padding: 0 !important;
}

button::after {
  border: none;
}

view[hidden] {
  display: none !important;
}

.noMoreTips {
  width: 100%;
  padding-top: 30rpx;
  font-size: 32rpx;
  /* color: #e91f5d; */
  color: #F29700;
  text-align: center;
  font-weight: bold;
}

.commonBg {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 184rpx;
  background-color: #18181d;
}

.commonLogo {
  position: absolute;
  top: 0;
  left: 0;
  width: 236rpx;
  height: 184rpx;
}

.warp {
  position: fixed;
  top: 184rpx;
  left: 0;
  right: 0;
  bottom: 155rpx;
  background-color: #18181d;
}

.commonBottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 750rpx;
  height: 155rpx;
  z-index: 999;
  background-color: #fff;
  border-radius: 28rpx 28rpx 0 0;
  box-shadow: 0 0 16rpx rgba(0, 0, 0, 0.3);
}

.commonBtn {
  margin: 15rpx auto;
  width: 320rpx;
  height: 74rpx;
  background: #43434a;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 74rpx;
  text-align: center;
  letter-spacing: 3rpx;
}

.commonTop {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 300rpx;
  background-color: #18181d;
}
.commonInfo {
  width: 100%;
  height: auto;
  padding: 30rpx;
  display: flex;
  align-items: center;
}

.commonIcon {
  width: 88rpx;
  height: 88rpx;
}

.commonWord {
  width: 550rpx;
  padding-left: 20rpx;
}
.commonLabel {
  max-width: 500rpx;
  font-size: 24rpx;
  color: #ffffff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.commonNum {
  padding-top: 12rpx;
  color: #ffffff;
  font-size: 40rpx;
  font-weight: bold;
}
.commonNum text {
  font-size: 24rpx;
}
.commonWrap {
  position: absolute;
  top: 160rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #ffffff;
  border-radius: 28px 28px 0px 0px;
}
.commonfilterBox {
  position: absolute;
  top: 80rpx;
  right: 30rpx;
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-end;
}
.commonfilterItem {
  width: auto;
  height: 40rpx;
  font-size: 26rpx;
  color: #ffffff;
  line-height: 40rpx;
  margin-right: 32rpx;
  position: relative;
}
.commonfilterItem text {
  position: relative;
  z-index: 9;
}
.commonfilterItem.act {
  font-size: 32rpx;
  font-weight: bold;
}
.commonfilterItem.act:after {
  content: "";
  position: absolute;
  bottom: -5rpx;
  left: 0;
  height: 16rpx;
  width: 100%;
  background-color: #e91f86;
}

.commonScrollBox {
  position: absolute;
  top: 50rpx;
  left: 0;
  bottom: 0;
  right: 0;
}
.commonRecordItem {
  width: 694rpx;
  height: 102rpx;
  background: #f4f4f4;
  margin: 0 auto 20rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.commonRecordinfo {
  width: 50%;
  height: 100%;
}
.commonRecordtype {
  width: 100%;
  height: 50%;
  color: #333333;
  font-size: 26rpx;
  line-height: 65rpx;
  font-weight: bold;
}
.commonRecordtype.sp {
  color: #e91f86;
}
.commonRecordtime {
  width: 100%;
  height: 50%;
  color: #18181d;
  font-size: 22rpx;
  line-height: 45rpx;
}
.commonRecordnum {
  width: 50%;
  height: 100%;
  text-align: right;
  font-size: 36rpx;
  color: #18181d;
  font-weight: bold;
  line-height: 102rpx;
}
.commonRecordnumIcon {
  width: 32rpx;
  height: 28rpx;
  margin-left: 3rpx;
  position: relative;
}

.commonfilterBox2 {
  width: 100%;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.commonfilterItem2 {
  width: 156rpx;
  height: 66rpx;
  position: relative;
  font-size: 24rpx;
  color: #ffffff;
  line-height: 66rpx;
  text-align: center;
  margin: 0 10rpx;
}
.commonfilterImg2 {
  position: absolute;
  top: 0;
  left: 0;
  width: 156rpx;
  height: 66rpx;
}
.commonfilterItem2 text {
  position: relative;
  z-index: 9;
}

.commonOrderItem {
  width: 680rpx;
  height: auto;
  border-bottom: 1px solid #999999;
  margin: 0 auto 60rpx;
}

.commonOrderinfoBox {
  width: 100%;
  overflow: hidden;
}

.commonOrderheadImg {
  float: left;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.commonOrderrow {
  float: right;
  width: 87%;
}

.commonOrderName {
  float: left;
  width: 50%;
  font-size: 28rpx;
  color: #333333;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.commonOrderNumBox {
  float: right;
  display: inline-block;
  width: auto;
  position: relative;
  background-color: #e91f83;
  height: 50rpx;
  top: 25rpx;
  right: 30rpx;
}
.commonOrderNumBox::before {
  content: "";
  position: absolute;
  bottom: 0;
  left: -25rpx;
  border: 10rpx solid #fff;
  border-width: 25rpx 25rpx 50rpx 25rpx;
  border-color: rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #e91f83 rgba(0, 0, 0, 0);
}
.commonOrderNumBox::after {
  content: "";
  position: absolute;
  top: 0;
  right: -25rpx;
  border: 10rpx solid #fff;
  border-width: 50rpx 25rpx 25rpx 25rpx;
  border-color: #e91f83 rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) rgba(0, 0, 0, 0);
}
.commonOrderNum {
  display: inline-block;
  font-size: 24rpx;
  color: #ffffff;
  padding-right: 32rpx;
  position: relative;
  z-index: 9;
  top: -6rpx;
}

.commonOrderPriceBox {
  position: relative;
  display: inline-block;
  font-size: 34rpx;
  font-weight: bold;
  color: #ffffff;
  text-align: right;
  height: 54rpx;
  line-height: 54rpx;
  padding: 0 16rpx;
  background-color: #000000;
  z-index: 9;
  top: -10rpx;
  right: 4rpx;
}
.commonOrderPriceBox text {
  font-size: 26rpx;
}
.commonOrderPriceBox::before {
  content: "";
  position: absolute;
  bottom: 0;
  left: -27rpx;
  border: 10rpx solid #fff;
  border-width: 27rpx 27rpx 54rpx 27rpx;
  border-color: rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #000000 rgba(0, 0, 0, 0);
}
.commonOrderPriceBox::after {
  content: "";
  position: absolute;
  top: 0;
  right: -27rpx;
  border: 10rpx solid #fff;
  border-width: 54rpx 27rpx 27rpx 27rpx;
  border-color: #000000 rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) rgba(0, 0, 0, 0);
}

.commonOrderCreatTime {
  font-size: 24rpx;
  color: #999999;
  line-height: 34rpx;
}

.commonOrderGoodsBox {
  width: 100%;
  height: 195rpx;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
}

.commonOrderGoodsScrollBox {
  width: 100%;
  height: 100%;
}

.commonOrderGoodItem {
  float: left;
  width: 160rpx;
  height: 190rpx;
  margin-right: 20rpx;
}

.commonOrderGoodItem:last-child {
  margin-right: 0;
}
.commonOrderGoodImg {
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
}
.commonOrderGoodWord {
  font-size: 20rpx;
  color: #333333;
  padding-top: 8rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.ml-20 {
  margin-left: 20rpx;
}
.mr-20 {
  margin-right: 20rpx;
}

/* 字体 */
.din {
  font-family: "DINAlternate-Bold";
}

/* 单行省略号 */
.text-hide {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
}

.commonWrapNew {
  position: absolute;
  top: 160rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background-color:#3C3C3C;
  border-radius: 40px 40px 0px 0px;
}
.commonHeadNew {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 148rpx;
  background-image: url('data:image/png;base64,');
}
/* 通过增加 uc-perf-stat-ignore:image style 来忽略其对T2计算的影响 */
.item-image {
  uc-perf-stat-ignore: image;
}