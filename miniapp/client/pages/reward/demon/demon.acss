page {
  background-color: #2C2C2C;
  overflow: hidden auto;
}

.demonContainer {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
}
.demonContainer .close {
  position: absolute;
  top: -60rpx;
  right: 18rpx;
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  border: 1rpx solid #fff;
  border-radius: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 48rpx;
  z-index: 9999;
}
.demonContainer .demonTop {
  /* padding: 0 26rpx; */
  text-align: center;
  color: #fff;
}
.demonTop .demonTopContent {
  position: relative;
  width: 750rpx;
  height: 670rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  padding-top: 250rpx;
}
.demonTop .demonKingHead {
  position: absolute;
  z-index: 9;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.demonTop .demonInfo {
  position: absolute;
  z-index: 99;
  display: flex;
  width: 100%;
  height: 300rpx;
  padding: 16rpx 22rpx 30rpx;
}
.demonTop .occupiedTime {
  position: absolute;
  top: 24rpx;
  right: 50rpx;
  width: 230rpx;
  color: #feeec2;
  font-size: 22rpx;
  text-align: center;
  overflow-x: auto;
}
.demonTop .avator {
  position: relative;
  top: 10rpx;
  left: 80rpx;
  width: 152rpx;
  height: 156rpx;
}
.demonTop .avator .avatorImg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-top: 16rpx;
}
.demonTop .avator .avatorKimg {
  position: absolute;
  z-index: 9;
  top: 0;
  left: 0;
  width: 152rpx;
  height: 156rpx;
}
.demonTop .avatorDetail {
  margin-top: 50rpx;
  margin-left: 100rpx;
  text-align: left;
  flex: 1;
}
.avatorDetail .nickName {
  font-size: 24rpx;
  font-weight: bold;
}
.avatorDetail .upgrade {
  margin-top: 18rpx;
  font-size: 24rpx;
  color: #ffab30;
}
.avatorDetail .numbers {
  margin-top: 34rpx;
}

.demonContainer .demonList {
  position: relative;
  top: -100rpx;
  width: 100%;
  height: 920rpx;
  /* padding: 0 44rpx 210rpx; */
  width: 100%;
}
.demonContainer .demonList .demonKingBg {
  position: absolute;
  z-index: 99;
  width: 100%;
  height: 100%;
}
.demonContainer .demonList .demonItemBox {
  position: absolute;
  z-index: 99;
  width: 100%;
  height: 920rpx;
  overflow: scroll;
  padding: 0 0 180rpx;
  margin-top: 14rpx;
}
.demonContainer .demonList .demonItem {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
  padding: 8rpx 18rpx 22rpx;
  color: #fff;
  /* background-image: linear-gradient(
    to right,
    transparent 70%,
    rgb(237, 30, 121, 0.5)
  ); */
}
.demonItem.bgBorder {
  background-image: url('data:image/png;base64,');
}
.demonItem .demonItemBg {
  position: absolute;
  top: 0;
  left: 32rpx;
  width: 64rpx;
  height: 134rpx;
  z-index: -1;
}
.demonList .demonItem .serialNumber {
  width: 92rpx;
  height: 60rpx;
}
.demonList .demonItem .rankTag {
  position: relative;
  width: 58rpx;
  height: 62rpx;
  margin: 0 18rpx;
  text-align: center;
  line-height: 62rpx;
  color: #CFB39E;
  font-size: 33rpx;
  background-image: url('data:image/png;base64,');
}

.demonContainer .demonBottom {
  position: absolute;
  bottom: 0;
  z-index: 99;
  width: 100%;
  height: 194rpx;
  display: flex;
  align-items: center;
  margin-top: 48rpx;
  border-radius: 30rpx 30rpx 0 0;
  padding: 36rpx;
  padding-left: 100rpx;
  color: #fff;
  background-image: url('data:image/png;base64,');
}
.demonBottom .serialNumber {
  position: absolute;
  top: 60rpx;
  left: 34rpx;
  width: 60rpx;
  font-size: 26rpx;
  text-align: center;
}
.mt-20 {
  margin-top: 20rpx;
}
.mt-12 {
  margin-top: 12rpx;
}
.ml-10 {
  margin-left: 10rpx;
}

.detail {
  margin-left: 24rpx;
}
.nameBox {
  width: 340rpx;
}
.detail .nickName {
  font-size: 28rpx;
  font-weight: bold;
  margin-right: 10rpx;
}
.demonList .occupiedNumber {
  position: absolute;
  font-size: 24rpx;
  top: 18rpx;
  right: 32rpx;
  color: #F7B52D;
}
.detail .numbers {
  margin-top: 12rpx;
}
.button {
  width: 180rpx;
  height: 48rpx;
  line-height: 48rpx;
  display: inline-block;
  margin-top: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  font-size: 20rpx;
  overflow: hidden;
  background-image: url('data:image/png;base64,');
}
.button-right {
  background-image: url('data:image/png;base64,');
}
.avatorImgs {
  position: relative;
  margin-left: 20rpx;
  margin-top: 20rpx;
  width: 99rpx;
  height: 99rpx;
  background-size: 100% 100%;
  border-radius: 50%;
}
.demonList .userBox {
  position: absolute;
  bottom: -44rpx;
  left: -28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: bolder;
  color: #fff;
  width: 140rpx;
}
.memberCardTag {
  width: 60rpx;
  height: 24rpx;
  margin-left: 10rpx;
}
.avatorBorder {
  position: relative;
  top: -8rpx;
  left: -8rpx;
  width: 100rpx;
  height: 100rpx;
}
.level {
  position: relative;
  bottom: 2rpx;
  margin-left: 6rpx;
  color: #fff;
  font-size: 20rpx;
  line-height: 28rpx;
}
.demonBottom .occupiedNumber {
  position: absolute;
  font-size: 24rpx;
  top: 12rpx;
  right: 24rpx;
  color: #F7B52D;
}
.d-flex {
  display: flex;
}
.a-center {
  align-items: center;
}
.userLevel {
  width: 76rpx;
  height: 22rpx;
  border-radius: 4rpx;
  font-size: 18rpx;
  font-weight: bold;
  text-align: center;
  line-height: 22rpx;
  color: #fceeab;
  font-style: italic;
  margin-left: 10rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
