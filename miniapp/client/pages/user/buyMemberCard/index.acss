page {
  color: #fff;
  background-image: linear-gradient(to bottom, #0f1322, #0f1322);
}
.buyMemberCardContainer {
  position: relative;
  height: 100vh;
  overflow: scroll;
  box-sizing: border-box;
  padding-bottom: 80rpx;
}
.buyMemberCardContainer .buyMembershipCardBg {
  width: 100%;
  height: 765rpx;
}
.buyMemberCardContainer .buyMemberCardContent {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  padding-top: 180rpx;
}
.buyMemberCardContainer .memberDescriptionBox {
  display: flex;
  justify-content: center;
  margin-top: 8rpx;
  width: 100%;
}
.buyMemberCardContainer .memberCardPrice {
  width: 222rpx;
  height: 72rpx;
  text-align: center;
  line-height: 92rpx;
  font-size: 32rpx;
  color: #000;
  font-weight: bold;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100% auto;

}
