.buyMemberCardContainer .membershipCardBox .swiper {
  width: 100%;
  /* height: 494rpx; */
}
.buyMemberCardContainer .membershipCardBox .swiperItem {
  display: flex;
  flex-direction: column;
  align-items: center;
}
/* 开卡权益 */
.buyMemberCardContainer .cardBenefitBox {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 48rpx;
}
.buyMemberCardContainer .cardBenefitBox .cardBenefit {
  width: 636rpx;
  height: 302rpx;
}
.buyMemberCardContainer .cardBenefitBox .benefitBg1 {
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.buyMemberCardContainer .cardBenefitBox .benefitBg2 {
  position: relative;
  top: -20rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.buyMemberCardContainer .cardBenefitBox .benefitTitle {
  padding: 24rpx 0;
  font-size: 28rpx;
  font-weight: bold;
  text-align: center;
  color: #fff;
}
.buyMemberCardContainer .cardBenefitBox .benefitScroll {
  display: flex;
  margin-top: 20rpx;
}

/* 购买操作栏 */
.buyMemberCardContainer .chooseBox {
  position: fixed;
  z-index: 9999;
  bottom: 0;
  width: 100%;
  height: 230rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100% auto;
}
.buyMemberCardContainer .chooseBox .chooseCardTextBox {
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
  color: #fff;
  font-size: 24rpx;
}
.buyMemberCardContainer .chooseBox .chooseCardText {
  width: 96rpx;
  height: 50rpx;
  line-height: 30rpx;
  text-align: center;
}
.buyMemberCardContainer .chooseBox .chooseCardOperateBox {
  display: flex;
  /* justify-content: space-between; */
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #FE9F00;
}
.buyMemberCardContainer .choosePrice {
  margin-right: 146rpx;
  font-weight: bolder;
  /* font-style: italic; */
}
.buyMemberCardContainer .chooseCardOperateBox .chooseDin {
  font-size: 28rpx;
}
.buyMemberCardContainer .chooseBox .buyBtn {
  margin-left: 146rpx;
  font-size: 28rpx;
}
.buyMemberCardContainer .setGray {
  filter: grayscale(100%);
}
