/* components/phoneBind/phoneBind.wxss */
.phoneBindMask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 10000;
    background-color: rgba(0, 0, 0, 0.8);
}
.dialog {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 498rpx;
    z-index: 10001;
    color: #fff;
    background-image: url('data:image/png;base64,');
}
.close {
    position: absolute;
    top: 40rpx;
    right: 40rpx;
}
.content {
    margin-top: 144rpx;
    padding: 0 54rpx 54rpx;
}
.title {
    width: 172rpx;
    height: 40rpx;
}
.tip {
    margin-top: 20rpx;
    line-height: 38rpx;
    font-size: 24rpx;
}
.buttons {
    display: flex;
    justify-content: center;
    font-size: 28rpx;
    color: #fff;
}
.button {
    display: inline-block;
    width: 248rpx;
    height: 66rpx;
    text-align: center;
    line-height: 66rpx;
}
.buttons button {
    color: #fff;
    font-size: 28rpx;
    line-height: 66rpx;
}
.cancel {
    background-image: url('data:image/png;base64,');
}
.ensure {
    margin-left: -26rpx;
    background-image: url('data:image/png;base64,')
}
