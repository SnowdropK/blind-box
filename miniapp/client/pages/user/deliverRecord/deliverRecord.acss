/* pages/user/deliverRecord/deliverRecord.wxss */
page {
  background-color: #3C3C3C;
  height: 100vh;
}
.deliverRecordContainer {
  height: 100vh;
}
.deliverRecordContainer .commonTop {
  position: relative;
}
.deliverRecordContainer .commonWrap {
  top: 240rpx !important;
  border-radius: 28rpx 28rpx 0 0;
}
.deliverRecordContainer .commonWrapNew {
  top: 251rpx;
  background-color: #3C3C3C;
}

.deliverRecordContainer .deliverFilterBox {
  display: flex;
  position: absolute;
  top: 210rpx;
  padding-left: 46rpx;
  color: #fff;
  font-size: 24rpx;
}
.deliverRecordContainer .deliverFilterItem {
  margin-right: 36rpx;
}
/* .filterItem text {
  position: relative;
  z-index: 9;
} */
.deliverRecordContainer .deliverFilterItem.deliverAct {
  color: #FFC000;
}

.deliverRecordContainer .deliverScrollBox {
  position: absolute;
  top: 50rpx;
  left: 0;
  bottom: 0;
  right: 0;
}

.deliverRecordContainer .deliverApplyItem {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 34rpx;
}

.deliverRecordContainer .deliverApplyItem .itemCard {
  width: 690rpx;
  height: 336rpx;
  /* padding: 30rpx; */
}
.deliverRecordContainer .deliverApplyItem .itemCardBg1 {
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.deliverRecordContainer .deliverApplyItem .itemCardBg2 {
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.deliverRecordContainer .deliverApplyItem .itemCardBg3 {
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.deliverRecordContainer .deliverApplyItem .itemCard .infoTime {
  position: relative;
  top: -14rpx;
  font-size: 24rpx;
  text-align: center;
}
.deliverRecordContainer .deliverApplyItem .recordsHeadBox {
  height: 32rpx;
  line-height: 32rpx;
  color: #000;
  font-size: 24rpx;
  text-align: center;
}

.deliverRecordContainer .deliverApplyItem .waiting {
  /* background-color: #f7b500; */
}
.deliverRecordContainer .deliverApplyItem .preparing {
  /* background-color: #f78300; */
}
.deliverRecordContainer .deliverApplyItem .delivered {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 58rpx;
}
.deliverRecordContainer .itemCardContent {
  padding: 50rpx 58rpx 0;
  color: #fff;
  font-size: 24rpx;
}
.deliverRecordContainer .infoTitle {
  margin-top: 52rpx;
}
.deliverRecordContainer .infoWord {
  margin-top: 16rpx;
  color: #8c8c8c;
  line-height: 1.2;
}
.deliverRecordContainer .goodsNum {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.deliverRecordContainer .deliverType1 {
  color: #FFA800;
}
.deliverRecordContainer .deliverType2 {
  color: #FF6D00;
}
.deliverRecordContainer .deliverType3 {
  color: #00FFB0;
}