.noticeBarBox {
  position: relative;
  padding: 22rpx 22rpx 36rpx 56rpx;
  margin-bottom: 30rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.noticeBarBox .noticePoint {
  position: absolute;
  top: 14rpx;
  left: 12rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 8rpx;
  background-color: #F1AB04;
}
.noticeBarBox .noticeHead {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 10rpx;
}
.noticeBarBox .noticeBarTitle {
  font-size: 30rpx;
  color: #F1AB04;
  font-weight: bold;
}
.noticeBarBox .noticeTime {
  font-size: 22rpx;
  color: #b3b3b3;
}
.noticeBarBox .noticeContentBox {
  margin-top: 28rpx;
}
.noticeBarBox .noticeImgBox {
  position: relative;
  border-radius: 8rpx;
  overflow: hidden;
}
.noticeBarBox .noticeImgBox .tipBar {
  position: absolute;
  bottom: 0;
  z-index: 99;
  width: 100%;
  height: 36rpx;
  padding-left: 32rpx;
  line-height: 36rpx;
  font-size: 18rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.6);
}
.noticeBarBox .noticeImg {
  /* width: 100%; */
  height: 178rpx;
  border: 1px solid red;
}
.noticeBarBox .noticeContentTipBox {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.noticeBarBox .noticeContent {
  width: 480rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #fff;
  line-height: 36rpx;
}
.noticeBarBox .noticeArrow {
  margin-top: 16rpx;
  width: 40rpx;
  height: 40rpx;
}
