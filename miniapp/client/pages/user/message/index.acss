page {
  background-color: #040D14;
}
.messageContainer {
  position: relative;
  height: 100vh;
  color: #fff;
  box-sizing: border-box;
}
.messageContainer .messageContentBox {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
}
.messageContainer .messageHeader {
  /* position: sticky;
  top: 0;
  z-index: 3; */
}
.messageContainer .noticeBox {
  display: flex;
  justify-content: center;
  width: 680rpx;
  height: 236rpx;
  padding: 42rpx 88rpx 0;
  font-size: 26rpx;
  color: #cccccc;
  font-weight: bold;
  text-align: center;
  box-sizing: border-box;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
  mix-blend-mode: screen;
}
.messageContainer .imgBox {
  position: relative;
}
.messageContainer .noticeLogo {
  width: 102rpx;
  height: 102rpx;
}
.messageContainer .noticeNum {
  position: absolute;
  top: -16rpx;
  right: -26rpx;
  width: 34rpx;
  height: 34rpx;
  border-radius: 21rpx;
  color: #000;
  line-height: 32rpx;
  background-color: #F1AB04;
}
.messageContainer .noticeTitle {
  margin-top: 16rpx;
  font-size: 22rpx;
}
.messageContainer .clearNoticeBox {
  position: relative;
  top: -10rpx;
  display: flex;
  flex-direction: row-reverse;
  padding-right: 46rpx;
}
.messageContainer .clearNoticeBox .clearNotice {
  width: 200rpx;
  height: 40rpx;
}
.messageContainer .messageScrollBox {
  margin-top: 12rpx;
  color: #000;
  /* border: 1px solid red; */
}
.messageContainer .messageBox {
  padding: 0 30rpx 20rpx;
  /* border: 1px solid red; */
}
.messageContainer .messageEmpty {
  margin-top: 160rpx;
  margin-left: 90rpx;
  width: 520rpx;
  height: 438rpx;
}