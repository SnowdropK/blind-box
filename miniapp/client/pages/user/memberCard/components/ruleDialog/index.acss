/* 弹窗 */
.dialogMak {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.8);
}
.dialog {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
  width: 90%;
  box-sizing: border-box;
  padding: 49rpx 46rpx 43rpx;
}
.dialog .button {
  margin-top: 36rpx;
  margin-left: 50%;
  transform: translate(-50%, 0);
  width: 180rpx;
  height: 80rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  background-image: linear-gradient(-90deg, #4a7cf7, #7da8f6);
}
.dialog .dialogTitle {
  font-size: 30rpx;
  text-align: center;
}
/* 规则弹窗 */
.dialogRuleBc {
  background-image: url("data:image/png;base64,");
}
.dialog .ruleText {
  max-height: 718rpx;
  min-height: 300rpx;
  font-size: 28rpx;
  line-height: 48rpx;
}
