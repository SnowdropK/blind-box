.personalCentreContainer {
  width: 750rpx;
  height: 100vh;
  /* position: relative; */
  padding-top: 258rpx;
  box-sizing: border-box;
  overflow: scroll;
}
.personalCentreContainer .memberBg {
  width: 750rpx;
  height: 528rpx;
}
.personalCentreContainer .memberContent {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
}
.personalCentreContainer .levelBox {
  position: relative;
  width: 750rx;
  height: 567rpx;
  box-sizing: border-box;
  color: #fff;
}
.personalCentreContainer .levelBox .memberBg {
  width: 750rx;
  height: 567rpx;
}
.personalCentreContainer .levelBox .levelContent {
  position: absolute;
  left: 0;
  top: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.personalCentreContainer .levelBox .avatorBox {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.personalCentreContainer .levelBox .avatorImgBox {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 146rpx;
  height: 138rpx;
  box-sizing: border-box;
}
.personalCentreContainer .levelBox .avatorImg {
  width: 110rpx;
  height: 110rpx;
}
.personalCentreContainer .levelBox .avatorLevelFrame {
  position: absolute;
  z-index: 99;
  width: 146rpx;
  height: 138rpx;
}
.personalCentreContainer .levelBox .avatorInfoBox {
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* height: 98rpx; */
  margin-top: 30rpx;
  text-align: center;
}
.personalCentreContainer .levelBox .avatorName {
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.personalCentreContainer .levelBox .levelInfoBox {
  display: flex;
  align-items: center;
  margin-top: 12rpx;
  font-size: 22rpx;
  font-weight: bold;
  text-align: center;
  font-style: italic;
}
.personalCentreContainer .levelBox .levelLogo {
  margin-right: 12rpx;
  width: 98rpx;
  height: 30rpx;
  font-size: 20rpx;
  line-height: 30rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.personalCentreContainer .levelBox .levelText {
  display: inline-block;
  width: 98rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.personalCentreContainer .levelBox .progressBox {
  margin-top: 34rpx;
  width: 476rpx;
}
.personalCentreContainer .levelBox .progressBarBox {
  position: relative;
  height: 12rpx;
  border-radius: 6rpx;
  background-color: #181818;
}
.personalCentreContainer .levelBox .progressBar {
  position: absolute;
  top: 0;
  left: 0;
  height: 12rpx;
  border-radius: 6rpx;
  background-image: linear-gradient(90deg, #FF7E00, #FFA800);
}
.personalCentreContainer .levelBox .progressInfo {
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
  height: 28rpx;
  font-style: italic;
  font-weight: bold;
  font-size: 20rpx;
  line-height: 28rpx;
}
.personalCentreContainer .levelBox .nextLevelText {
  padding-top: 8rpx;
  color: #000;
  font-size: 22rpx;
  text-align: center;
}

/* 升级奖励 */
.personalCentreContainer .allPrizeBox {
  display: flex;
  justify-content: center;
  margin-top: 56rpx;
}
.personalCentreContainer .allPrizeBox .allPrizeBg {
  width: 660rpx;
  height: 630rpx;
}
.personalCentreContainer .allPrizeBox .allPrizeContent {
  position: absolute;
  top: 0;
  left: 0;
  width: 660rpx;
  height: 630rpx;
}
.personalCentreContainer .upgradeRewardBox {
  /* margin-top: 40rpx;
  box-sizing: border-box;
  padding: 0 50rpx; */
  padding-bottom: 30rpx;
}
.personalCentreContainer .upgradeRewardContent {
  /* padding: 14rpx 22rpx; */
  /* background-color: rgba(64, 49, 81, 0.8); */
}
.personalCentreContainer .upgradeRewardContent .title {
  color: #FFFFFF;
  width: 660rpx;
  height: 72rpx;
  box-sizing: border-box;
  padding-left: 32rpx;
  line-height: 72rpx;
  text-align: left;
  font-size: 28rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.personalCentreContainer .upgradeRewardContent .upgradationTitle {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 660rpx;
  height: 72rpx;
  box-sizing: border-box;
  padding-left: 32rpx;
  line-height: 72rpx;
  text-align: left;
  font-size: 28rpx;
  color: #F3AC00;
  background-image: linear-gradient(to right, #222a4e, rgba(0,0,0,0));
}
/* .personalCentreContainer .upgradeRewardContent .receiveBtnBox {
  display: flex;
  justify-content: center;
  margin-top: 16rpx;
} */
.personalCentreContainer .upgradeRewardContent .receiveBtn {
  width: 150rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: bold;
  color: #000000;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.personalCentreContainer .upgradeRewardContent .receiveBtnDisable {
  color: #3c3c46;
  background-color: #939393;
}
.personalCentreContainer .prizeScroll {
  margin-top: 26rpx;
}
.personalCentreContainer .levelPrizeBox {
  display: flex;
  align-items: center;
  height: 174rpx;
}
.personalCentreContainer .avatorBorderBox {
  margin-top: 34rpx;
  padding: 0 60rpx;
}
.personalCentreContainer .avatorBorderList {
  display: grid;
  grid-gap: 32rpx 0;
  justify-content: space-between;
  grid-template-columns: repeat(5, 20%);
  justify-items: center;
  margin-top: 34rpx;
}
.personalCentreContainer .avatorBorderImgBox {
  width: 98rpx;
}
.personalCentreContainer .avatorBorderImg {
  width: 98rpx;
  height: 104rpx;
}
.personalCentreContainer .avatorBorderText {
  margin-top: 4rpx;
  text-align: center;
  color: #f5d5a3;
  font-size: 20rpx;
}
.personalCentreContainer .ruleDescriptionBox {
  margin-top: 30rpx;
  padding: 0 70rpx 50rpx;
  color: #F3AC00;
  font-size: 24rpx;
}
.personalCentreContainer .ruleDescriptionBox .content {
  margin-top: 20rpx;
  display: inline-block;
  line-height: 32rpx;
  color: #BBB;
  font-size: 22rpx;
}
