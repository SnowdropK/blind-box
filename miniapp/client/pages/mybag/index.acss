}
/* 内容 */
.myBagContainer .myBagBox {
  position: absolute;
  /* flex: 1; */
  top: 240rpx;
  /* padding: 30rpx 38rpx;  */
  width: 100%;
}
.myBagContainer .myBagBox .myBagBg {
  width: 100%;
}
.myBagContainer .myBagBox .myBagContentBox {
  position: absolute;
