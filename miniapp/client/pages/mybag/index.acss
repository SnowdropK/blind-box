}
.myBagContainer .bagGoodInfo:nth-child(4n) {
  margin-right: 0;
}
.myBagContainer .bagGoodImgBox {
  position: relative;
}
.myBagContainer .bagGoodImg {
  width: 150rpx;
  height: 150rpx;
  box-sizing: border-box;
}
