}
.myBagContainer .filterBox .filterLeftBox {
  display: flex;
  align-items: center;
  margin-left: 78rpx;
}
.myBagContainer .filterBox .resellLabel {
  margin-left: 110rpx;
}
.myBagContainer .deliverRuleBtn {
  position: absolute;
  left: 210rpx;
  top: 50rpx;
  font-size: 28rpx;
  color: #f9f9f9;
  display: flex;
  align-items: center;
  z-index: 10;
}
