/* 操作栏 */
.myBagContainer .operateBox {
  position: fixed;
  bottom: 0;
  z-index: 99999;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  font-size: 30rpx;
  color: #fff;
}
.myBagContainer .operate {
  position: relative;
}
.myBagContainer .operateText {
  position: absolute;
  z-index: 9;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 310rpx;
  height: 114rpx;
}
.myBagContainer .shelfBtn {
  width: 310rpx;
  height: 114rpx;
}
.myBagContainer .deliverBtn {
  width: 310rpx;
  height: 114rpx;
}
.showing {
  -webkit-animation: showing 0.2s linear forwards;
}
@-webkit-keyframes showing {
  0% {
    transform: translate(450rpx, 0);
  }
  100% {
    transform: translate(0, 0);
  }
}
