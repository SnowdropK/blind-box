.myBagContainer .choseAllBox {
  position: relative;
  bottom: -2rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.myBagContainer .choseAllBox.sp {
  color: #F69000;
}
.myBagContainer .choseAllIcon {
  margin-right: 10rpx;
  width: 36rpx;
  height: 36rpx;
  border-radius: 18rpx;
  background-color: #4d4d4d;
}
.myBagContainer .choseAllIcon_a {
  display: flex;
  justify-content: center;
  align-items: center;
  border: 2px solid #4d4d4d;
  background-color: #1F1A12;
}
.myBagContainer .choseAllIcon_a::before {
  content: "";
