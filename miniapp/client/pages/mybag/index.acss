/* pages/mybag/mybag.wxss */
page {
  background-color: #3C3C3C;
}
.myBagContainer {
  /* display: flex;
  flex-direction: column; */
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  color: #fff;
}
/* 头部 */
.myBagContainer .bagHeadBox {
  width: 750rpx;
  height: 460rpx;
}
