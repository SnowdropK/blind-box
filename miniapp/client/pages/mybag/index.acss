}
/* 内容 选中区 */
.myBagContainer .myBagChoseBox {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-weight: bold;
  color: #fff;
}
.myBagContainer .myBagChoseBox .bagfilterItem {
  margin-right: 30rpx;
  font-size: 26rpx;
}
.myBagContainer .myBagChoseBox .bagfilterItem.act {
  color: #F69000;
  text-decoration: underline;
}
.myBagContainer .myBagChoseBox .bagfilterItem.act:after {
  content: "";
  position: absolute;
  bottom: -16rpx;
  left: 0;
  height: 4rpx;
  width: 100%;
  background-color: #F69000;
}
