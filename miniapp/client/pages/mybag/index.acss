  color: #F69000;
}
.myBagContainer .choseNum.sp {
  text-align: right;
}
.myBagContainer .choseNum text {
  font-size: 30rpx;
  color: #fff;
  font-weight: bold;
}
/* 商品区 */
.myBagContainer .goodsScrollBox {
  /* flex: 1; */
  margin-top: 26rpx;
  padding-bottom: 160rpx; 
  box-sizing: border-box;
}
.myBagContainer .block {
  color: #F69000;
  font-size: 24rpx;
