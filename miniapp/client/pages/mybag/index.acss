  /* padding: 0 38rpx;
  background-image: linear-gradient(to bottom, #dedede, transparent 24%); */
}
.myBagContainer .titleBox {
  width: 100%;
  padding: 22rpx 0 30rpx;
  overflow: hidden;
}
.myBagContainer .titleBox .bagDivider {
  height: 24rpx;
  width: 100%;
}
.myBagContainer .titleBox .myBagTitle {
  float: left;
  line-height: 40rpx;
  /* padding-left: 20rpx; */
  width: 270rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.myBagContainer .ar {
  float: right;
  width: 40rpx;
  height: 40rpx;
}
.myBagContainer .ar.act {
  transform: rotate(90deg);
}
.myBagContainer .myBagNum {
  float: right;
  line-height: 40rpx;
  padding-right: 10rpx;
}

.myBagContainer .myBagNum text {
  color: #fff;
  font-size: 28rpx;
  font-weight: bold;
}
