.ipBox .leiIcon {
  width: 144rpx;
  height: 38rpx;
  position: relative;
  top: -10rpx;
}

.ipBox .icon-margin {
  margin-left: 8rpx;
}

.ipBox .newIcon {
  width: 88rpx;
  height: 36rpx;
  margin-left: 8rpx;
  position: relative;
  top: -10rpx;
  right: 12rpx;
}
.ipBox .saleOpenIcon {
  width: 74rpx;
  height: 36rpx;
  margin-left: 8rpx;
  position: relative;
  top: -10rpx;
  right: 12rpx;
}

.ipBox .moving {
  -webkit-animation: moving 2.5s linear infinite;
}

@-webkit-keyframes moving {
  0% {
    transform: translate(0, 0);
  }

  10% {
    transform: translate(0, 10rpx);
  }

  20% {
    transform: translate(0, 0);
  }

  30% {
    transform: translate(0, 10rpx);
  }

  40% {
    transform: translate(0, 0);
  }

  100% {
    transform: translate(0, 0);
  }
}

.ipBox .ipImg {
  width: 100%;
  height: 336rpx;
  border-radius: 24rpx 24rpx 0 0;
}
.ipBox .ipInfoBox {
  height: 104rpx;
  padding: 12rpx 10rpx 14rpx 20rpx;
  box-sizing: border-box;
  border-radius: 0 0 24rpx 24rpx;
}
.ipBox .ipName {
  /* margin-top: 12rpx; */
  width: 100%;
  color: #fff;
  font-size: 24rpx;
}
.ipBox .ipDataBox {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}
.ipBox .ipType {
  position: relative;
  left: -12rpx;
  width: 108rpx;
  height: 46rpx;
  color: #fff;
  border-radius: 0 23rpx 23rpx 0;
  font-size: 18rpx;
  text-align: center;
  line-height: 46rpx;
  background-color: #000;
}
.ipBox .ipTypeBg0 {
  background-color: #2d2d2c;
}

.ipBox .ipTypeBg1 {
  background-color: #19a66c;
}

.ipBox .ipTypeBg2 {
  background-color: #e78526;
}

.ipBox .ipTypeBg3 {
  background-color: #7a3bdd;
}
.ipBox .ipIcon {
  width: 26rpx;
  height: 36rpx;
}
.ipBox .ipPriceInfo {
  display: flex;
  align-items: baseline;
  font-size: 22rpx;
  color: #F29700;
}
.ipBox .ipPriceInfo .sp {
  font-weight: bold;
}
.ipBox .ipPriceInfo .priceNum {
  font-size: 36rpx;
  font-weight: bolder;
}
.ipBox .ipRealeseBox {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 12rpx;
  font-size: 16rpx;
  padding: 0 4rpx;
}
.ipBox .ipRealeseText {
  color: #ed1e79;
}
.ipBox .ipHot {
  display: flex;
  align-items: center;
  font-size: 18rpx;
  color: #fff;
}
.ipBox .ipHot .ipHotNum {
  position: relative;
  top: 10rpx;
}
.ipBox .fireImg {
  margin-left: 6rpx;
  width: 20rpx;
  height: 46rpx;
}
.ipBox .ipRealeseTime {
  display: flex;
  align-items: center;
}
.ipBox .calanderLogo {
  width: 33rpx;
  height: 33rpx;
}

.text-hide {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
