.ipBox .tagBox {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 999;
  display: flex;
  justify-content: flex-end;
}
.ipBox .ipInfoBorder {
  position: absolute;
  top: 0;
  left: 0;
  width: 336rpx;
  height: 336rpx;
  z-index: 99;
}
.ipBox .ipInfoBorder .ipInfoBorderOccupiedBox {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  height: 60rpx;
  width: 282rpx;
  padding-left: 10rpx;
}
.ipBox .ipInfoBorder .ipInfoBorderAvator {
  /* margin-top: 8rpx;
  margin-left: 10rpx; */
  width: 46rpx;
  height: 46rpx;
  border-radius: 23rpx;
}
.ipBox .ipInfoBorder .ipInfoBorderOccupiedInfo {
  margin-left: 10rpx;
  color: #fff;
  font-size: 18rpx;
}
.ipBox .ipInfoBorder .ipInfoBorderTop {
  width: 282rpx;
  height: 60rpx;
}
.ipBox .ipInfoBorder .ipInfoBorderCenter {
  width: 336rpx;
  height: 212rpx;
}
.ipBox .ipInfoBorder .ipInfoBorderBottomBox {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: center;
}
.ipBox .ipInfoBorderBottomBox .bottomImg {
  width: 256rpx;
  height: 46rpx;
}
.ipBox .ipInfoBorderBottomBox .pointIcon {
}
