.prizeBox .couponNum {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 99;
  width: 46rpx;
  height: 46rpx;
  color: #000;
  font-size: 22rpx;
  font-weight: bold;
  text-align: center;
  line-height: 46rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}

