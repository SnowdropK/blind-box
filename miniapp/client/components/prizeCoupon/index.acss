.prizeBox {
  position: relative;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
  width: 184rpx;
  height: 192rpx;
  color: #fff;
  font-size: 22rpx;
  padding-top: 22rpx;
  box-sizing: border-box;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.prizeBox .discount {
  font-size: 80rpx;
  font-weight: bolder;
}
.prizeBox .sub {
  margin-top: 14rpx;
  width: 166rpx;
  color: #F3AC00;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
