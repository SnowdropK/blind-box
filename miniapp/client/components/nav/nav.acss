/* components/nav/nav.acss */
.nav-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 221.33rpx;
  background-image: url('data:image/png;base64,');
  z-index: 999;
}
.bgBack {
  /* background: linear-gradient(#2D2D2D,#000000); */
  background: linear-gradient(#2e2e2e 0%, #201613 100%);
  height: 420rpx;
  z-index: 9;
}

.content {
  position: relative;
  width: 100%;
  height: 44px;
}

.back {
  width: 52rpx;
  height: 46rpx;
}

.home {
  width: 46rpx;
  height: 46rpx;
  margin-left: 15rpx;
}

.lineblack {
  height: 36rpx;
  background: #b0b0b0;
  width: 1rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  opacity: 0.5;
}

.lineWhite {
  height: 36rpx;
  background: rgba(255, 255, 255, 0.7);
  width: 1rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  opacity: 0.5;
}

.title {
  text-align: center;
  /* font-weight: 900; */
  font-size: 30rpx;
}

.letBtnBox {
  width: 100px;
  display: flex;
  align-items: center;
}

.capsule-box {
  width: 90px;
  border-radius: 88rpx;
  /* background: #FFFFFF; */
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}

.isWhite {
  border: 1rpx solid rgba(255, 255, 255, 0.25);
  background: rgba(0, 0, 0, 0.15);
}

.isblack {
  /* border: 1rpx solid rgba(0, 0, 0, .1); */
  /* background: rgba(255, 255, 255, .6); */
}

.opacity {
  background: rgba(0, 0, 0, 0.1);
}

.capsult-item {
  display: flex;
  width: 50%;
  margin-left: 10rpx;
  align-items: center;
  justify-content: center;
}
.noBack {
  width: 42px;
}
.noBack .capsult-item {
  width: 100%;
}
